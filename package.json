{
  "name": "chat-academico",
  "version": "1.0.0",
  "description": "Chat educacional utilizando IA para auxílio a alunos universitários",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "init-db": "cd backend && npm run init-db"
  },
  "keywords": [
    "chat",
    "education",
    "openai",
    "gpt"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.1"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "chartjs-node-canvas": "^5.0.0",
    "pdf-parse": "^1.1.1"
  }
}
